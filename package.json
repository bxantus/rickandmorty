    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.3",
    "gh-pages": "^6.1.1",
    "typescript": "^5.4.5"
  }
}
